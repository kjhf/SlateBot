<?xml version="1.0" encoding="UTF-8"?>
<Root>
  <CommandType>BaseChange</CommandType>
  <Aliases>b264,b2b64,b2base64,bin2base64</Aliases>
  <Help>Changes a binary number into a base 64 number.</Help>
  <Module>Number Base Commands</Module>
  <Examples>!b264 1001\r\n!b264 0 100 101 110101</Examples>
  <ExtraData>
    <FromBase>2</FromBase>
    <ToBase>64</ToBase>
  </ExtraData>
</Root>