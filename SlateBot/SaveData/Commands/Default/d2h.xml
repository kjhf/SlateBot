<?xml version="1.0" encoding="UTF-8"?>
<Root>
  <CommandType>BaseChange</CommandType>
  <Aliases>d2h,hex,hexadecimal,dec2hex</Aliases>
  <Help>Changes a decimal number into a hexadecimal number.</Help>
  <Module>Number Base Commands</Module>
  <Examples>!hex 100\r\n!d2h 0 255 160 119</Examples>
  <ExtraData>
    <FromBase>10</FromBase>
    <ToBase>16</ToBase>
  </ExtraData>
</Root>